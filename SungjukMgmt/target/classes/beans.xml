<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="chulsu" class="com.example.StudentVO">
		<!-- Setter Injection -->
		<property name="hakbun" value="2020-01" />
		<property name="name" value="김철수" />
		<property name="kor" value="90" />
		<property name="eng">
			<value>89</value>
		</property>
		<property name="mat" value="77" />
	</bean>
	<bean id="younghee" class="com.example.StudentVO">
		<!-- Setter Injection -->
		<property name="hakbun" value="2020-02" />
		<property name="name" value="박영희" />
		<property name="kor" value="90" />
		<property name="eng">
			<value>10</value>
		</property>
		<property name="mat" value="100" />
	</bean>
	<bean id="myCalculator" class="com.example.Calculator">
		<property name="student" ref="younghee"></property>
	</bean>

</beans>
